@using AttendenceSystem.Models;
@model IEnumerable<Department>

@{
    ViewBag.Title = "Department List";
}
<div style="font-family:Arial">

    <h2>Department List</h2>
    <ul>
        @foreach(Department departments in @Model)
        {

            <li>
                @Html.ActionLink(departments.DeptName, "YearTermList", "YearTerm", new { id =departments.DeptId} ,null)
            </li>
        }
    </ul>
</div>

